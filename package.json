{
  "name": "gql-llm",
  "version": "0.0.0",
  "description": "GQL LLM",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/maxneuvians/graphql-llm-query",
  "bugs": "https://github.com/maxneuvians/graphql-llm-query/issues",
  "repository": "maxneuvians/graphql-llm-query",
  "license": "MIT",
  "author": "Max Neuvians @maxneuvians",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gql-llm": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "yarn lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "gql-llm",
    "commands": "./dist/commands",
    "dirname": "gql-llm",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@graphql-tools/load": "^8.0.2",
    "@graphql-tools/url-loader": "^8.0.2",
    "@langchain/cohere": "^0.0.8",
    "@langchain/community": "^0.0.54",
    "@langchain/core": "^0.1.61",
    "@langchain/openai": "^0.0.28",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chromadb": "^1.8.1",
    "gql-generator": "^2.0.0",
    "graphql": "^16.8.1",
    "langchain": "^0.1.36"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "langchain-core": "^0.0.4",
    "mocha": "^10",
    "oclif": "^4.11.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
